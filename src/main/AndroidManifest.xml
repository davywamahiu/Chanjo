<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.desagu">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/chanjo_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddChildActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".DoctorDashboardActivity" />
        <activity android:name=".AddNotificationsActivity" />
        <activity android:name=".ImmunizationActivity" />
        <activity
            android:name=".child_history"
            android:label="@string/title_activity_child_history"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NotificationsActivity" />
        <activity android:name=".ProgressBarActivity" />
        <activity android:name=".MoreAboutVaccinesActivity" />
        <activity android:name=".VaccinesDescriptionActivity" />
        <activity android:name=".ReportIssueActivity" />
        <activity android:name=".noti_description" />
        <activity android:name=".RegisteredActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".ViewCaseActivity" />
        <activity android:name=".Help"></activity>
    </application>

</manifest>